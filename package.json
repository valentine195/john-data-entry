{
	"name": "packaging-plus",
	"productName": "Packaging Plus Invoice Entry",
	"author": "Jeremy Valentine",
	"description": "Invoice entry application for John",
	"version": "2.1.3",
	"private": true,
	"scripts": {
		"serve": "vue-cli-service serve",
		"build": "vue-cli-service build",
		"lint": "vue-cli-service lint",
		"electron:build": "cross-env NODE_ENV=production vue-cli-service electron:build --win -p always",
		"build-win": "electron-builder --win -p always",
		"electron:serve": "cross-env NODE_ENV=development vue-cli-service electron:serve",
		"postinstall": "electron-builder install-app-deps",
		"postuninstall": "electron-builder install-app-deps",
		"electron:generate-icons": "electron-icon-builder --input=./public/icon.png --output=build --flatten"
	},
	"main": "background.js",
	"dependencies": {
		"@fortawesome/fontawesome-free": "^5.13.0",
		"@octokit/request": "^5.4.9",
		"bootstrap": "^4.4.1",
		"core-js": "^3.6.5",
		"dotenv": "^8.2.0",
		"electron-log": "^4.2.4",
		"electron-updater": "^4.3.4",
		"esm": "^3.2.25",
		"exceljs": "^4.1.1",
		"fs-jetpack": "^3.1.0",
		"jquery": "^3.5.1",
		"moment": "^2.27.0",
		"popper.js": "^1.16.1",
		"v-calendar": "^1.0.0-beta.23",
		"vue": "^2.6.11",
		"vue-notification": "^1.3.20",
		"vue-router": "^3.1.6"
	},
	"devDependencies": {
		"@vue/cli-plugin-babel": "~4.5.0",
		"@vue/cli-plugin-eslint": "~4.5.0",
		"@vue/cli-service": "~4.5.0",
		"babel-eslint": "^10.1.0",
		"cross-env": "^7.0.2",
		"electron": "^9.0.0",
		"electron-devtools-installer": "^3.1.0",
		"electron-icon-builder": "^1.0.2",
		"eslint": "^6.7.2",
		"eslint-plugin-vue": "^6.2.2",
		"pug": "^3.0.0",
		"pug-plain-loader": "^1.0.0",
		"vue-cli-plugin-electron-builder": "~2.0.0-rc.4",
		"vue-template-compiler": "^2.6.11"
	}
