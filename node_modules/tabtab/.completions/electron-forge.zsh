###-begin-electron-forge-completion-###
if type compdef &>/dev/null; then
  _electron-forge_completion () {
    local reply
    local si=$IFS

    IFS=$'\n' reply=($(COMP_CWORD="$((CURRENT-1))" COMP_LINE="$BUFFER" COMP_POINT="$CURSOR" electron-forge completion -- "${words[@]}"))
    IFS=$si

    _describe 'values' reply
  }
  compdef _electron-forge_completion electron-forge
fi
###-end-electron-forge-completion-###
